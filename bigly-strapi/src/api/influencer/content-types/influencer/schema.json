{
  "kind": "collectionType",
  "collectionName": "influencers",
  "info": {
    "singularName": "influencer",
    "pluralName": "influencers",
    "displayName": "Influencer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "summary": {
      "type": "string"
    },
    "topics": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "politics",
        "news",
        "lifestyle",
        "history",
        "military"
      ]
    },
    "profile_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "internal_notes": {
      "type": "text"
    },
    "Profiles": {
      "type": "component",
      "component": "common.social-profile",
      "repeatable": true
    },
    "direct_contact_info": {
      "type": "component",
      "component": "common.contact-info",
      "repeatable": false
    },
    "blocklist_status": {
      "type": "component",
      "component": "subject.social-blocklist",
      "repeatable": true
    }
  }
}
