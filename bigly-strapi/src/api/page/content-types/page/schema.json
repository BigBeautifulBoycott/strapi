{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "headerImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "SEO": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "shared.slider",
        "shared.rich-text",
        "shared.quote",
        "shared.media"
      ]
    }
  }
}
